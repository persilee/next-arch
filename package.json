{
  "name": "nuxt-app",
  "private": true,
  "packageManager": "pnpm@9.15.4",
  "type": "module",
  "scripts": {
    "build": "nuxt build",
    "dev": "nuxt dev",
    "generate": "nuxt generate",
    "preview": "nuxt preview",
    "postinstall": "nuxt prepare",
    "sdb": "dotenv -e .env -- surreal start",
    "start:dev": "turbo dev sdb",
    "sdb:schema": "dotenv -e .env --  surreal import ./src/schema/surreal/app.surql -e http://localhost:3303",
    "sdb:demo": "dotenv -e .env --  surreal import ./src/schema/surreal/demo.surql -e http://localhost:3303",
    "sdb:update": "turbo sdb:schema"
  },
  "dependencies": {
    "@nuxt/ui": "^2.21.0",
    "@pinia/nuxt": "^0.9.0",
    "nuxt": "^3.15.2",
    "pinia": "^2.3.1",
    "surrealdb": "^1.1.0",
    "vue": "latest",
    "vue-router": "latest",
    "zod": "^3.24.1"
  },
  "devDependencies": {
    "@iconify-json/ri": "^1.2.5",
    "@iconify-json/solar": "^1.2.2",
    "dotenv-cli": "^8.0.0",
    "turbo": "^2.4.0"
  }
}
