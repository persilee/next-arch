{
  "name": "nuxt-app",
  "private": true,
  "packageManager": "pnpm@9.15.4",
  "type": "module",
  "scripts": {
    "build": "nuxt build",
    "dev": "nuxt dev",
    "generate": "nuxt generate",
    "preview": "nuxt preview",
    "postinstall": "nuxt prepare",
    "sdb": "dotenv -e .env -- surreal start",
    "start:dev": "turbo dev sdb redis:dev",
    "redis:dev": "redis-server /etc/redis.conf",
    "sdb:schema": "dotenv -e .env --  surreal import ./src/schema/surreal/app.surql -e http://localhost:3303",
    "sdb:test": "dotenv -e .env --  surreal import ./src/schema/surreal/test.surql -e http://localhost:3303",
    "sdb:update": "turbo sdb:schema"
  },
  "dependencies": {
    "@alicloud/dm20151123": "^1.2.4",
    "@alicloud/dysmsapi20170525": "^3.1.1",
    "@alicloud/openapi-client": "^0.4.12",
    "@alicloud/tea-typescript": "^1.8.0",
    "@alicloud/tea-util": "^1.4.10",
    "@casl/ability": "^6.7.3",
    "@nuxt/ui": "^2.21.0",
    "@pinia/nuxt": "^0.9.0",
    "argon2": "^0.41.1",
    "bree": "^9.2.4",
    "bullmq": "^5.40.2",
    "cheerio": "^1.0.0",
    "glob-to-regexp": "^0.4.1",
    "jsonwebtoken": "^9.0.2",
    "lodash-es": "^4.17.21",
    "nuxt": "^3.15.2",
    "pinia": "^2.3.1",
    "puppeteer": "^24.2.0",
    "qs": "^6.14.0",
    "surrealdb": "^1.1.0",
    "vue": "latest",
    "vue-router": "latest",
    "ws": "^8.18.0",
    "zod": "^3.24.1"
  },
  "devDependencies": {
    "@iconify-json/ri": "^1.2.5",
    "@iconify-json/solar": "^1.2.2",
    "@types/glob-to-regexp": "^0.4.4",
    "@types/jsonwebtoken": "^9.0.8",
    "@types/lodash-es": "^4.17.12",
    "@types/qs": "^6.9.18",
    "@types/ws": "^8.5.14",
    "dotenv-cli": "^8.0.0",
    "turbo": "^2.4.0"
  }
}
